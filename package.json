{
  "name": "obsidian-publisher",
  "description": "Easily publish your content",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "SÃ©bastien Dubois",
    "email": "sebastien@developassion.be",
    "url": "https://github.com/dsebastien/obsidian-publisher"
  },
  "scripts": {
    "postinstall": "npm run build:executor",
    "format": "nx format --all",
    "format:all": "npm run format",
    "lint": "nx lint",
    "build": "npm run build:dev",
    "serve": "npm run start",
    "start": "nx serve",
    "build:executor": "npx tsc tools/executors/esbuild/impl",
    "prepare:dist": "npm run tsc && npx rimraf ./dist/apps/obsidian-publisher && npx mkdirp ./dist/apps/obsidian-publisher && npm run version && npm run copy:assets",
    "prebuild:dev": "npm run prepare:dist",
    "build:dev": "nx build && touch ./dist/apps/obsidian-publisher/.hotreload && npx mkdirp ${OBSIDIAN_VAULT_LOCATION}/.obsidian/plugins/obsidian-publisher && cp -r ./dist/apps/obsidian-publisher/ ${OBSIDIAN_VAULT_LOCATION}/.obsidian/plugins/",
    "prebuild:prod": "npm run prepare:dist",
    "build:prod": "nx build --prod",
    "prewatch": "npm run prepare:dist",
    "watch": "nx watch --watch",
    "copy:assets": "cp -R ./apps/obsidian-publisher/src/assets/* ./dist/apps/obsidian-publisher/",
    "test": "nx test",
    "tsc": "tsc --noEmit --project ./apps/obsidian-publisher/tsconfig.app.json",
    "tsc:watch": "npm run tsc -- --watch",
    "tscw": "npm run tsc:watch",
    "update": "nx migrate latest",
    "migrate": "npx nx migrate --run-migrations",
    "version": "node ./version-bump.mjs && git add apps/obsidian-publisher/src/assets/manifest.json apps/obsidian-publisher/src/assets/versions.json"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/cli": "14.7.8",
    "@nrwl/eslint-plugin-nx": "14.7.8",
    "@nrwl/jest": "14.7.8",
    "@nrwl/linter": "14.7.8",
    "@nrwl/node": "14.7.8",
    "@nrwl/nx-cloud": "14.6.2",
    "@nrwl/workspace": "14.7.8",
    "@types/axios": "0.14.0",
    "@types/jest": "28.1.8",
    "@types/jsonwebtoken": "8.5.8",
    "@types/marked": "4.0.3",
    "@types/node": "18.7.1",
    "@types/object-hash": "2.2.1",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "esbuild": "0.18.10",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "28.1.3",
    "nx": "14.7.8",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@tryghost/admin-api": "1.13.1",
    "builtin-modules": "3.3.0",
    "cloudinary": "1.37.3",
    "gray-matter": "4.0.3",
    "immer": "9.0.21",
    "jsonwebtoken": "8.5.1",
    "marked": "5.1.0",
    "marked-base-url": "1.0.1",
    "object-hash": "3.0.0",
    "obsidian": "1.2.8",
    "tslib": "2.4.0",
    "zod": "3.21.4"
  }
}
